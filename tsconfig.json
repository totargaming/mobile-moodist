{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./rn-src",
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["@types/uuid"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "App.tsx",
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "src"
  ]
}